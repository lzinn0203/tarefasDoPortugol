programa {
  inteiro numeros[5]
  funcao inicio() {
    para(inteiro i = 0; i<5; i++){
      escreva("Digite um número: " )
      leia(numeros[i])
    }

escreva("Os números amarzenados: \n")

    para(inteiro i = 0; i<5; i++){
      escreva("Posição ", i, ": ", numeros[i], "\n")
    }
  }
}


