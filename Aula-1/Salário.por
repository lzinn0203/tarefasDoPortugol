programa {
  funcao inicio() {
    real valorHora, horasTrabalhadas, resultadoSalario

escreva("Digite o valor que você ganha por hora: ")
leia(valorHora)

escreva("Digite o número de horas trabalhadas por mês: ")
leia(horasTrabalhadas)

resultadoSalario = valorHora * horasTrabalhadas
escreva("O salário total no mês recorrente as horas trabalhadas é de: R$",resultadoSalario)


 }
}
